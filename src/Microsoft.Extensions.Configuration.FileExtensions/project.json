{
  "version": "1.2.0-*",
  "buildOptions": {
    "warningsAsErrors": true,
    "keyFile": "../../tools/Key.snk",
    "xmlDoc": true
  },
  "description": "Extension methods for configuring file-based configuration providers for Microsoft.Extensions.Configuration.",
  "packOptions": {
    "repository": {
      "type": "git",
      "url": "https://github.com/aspnet/configuration"
    },
    "tags": [
      "configuration"
    ]
  },
  "dependencies": {
    "Microsoft.Extensions.Configuration": {
      "target": "project"
    },
    "Microsoft.Extensions.FileProviders.Physical": "1.2.0-*",
    "NETStandard.Library": "1.6.2-*"
  },
  "frameworks": {
    "net451": {
      "frameworkAssemblies": {
        "System.Collections": {
          "type": "build"
        }
      }
    },
    "netstandard1.3": {
      "dependencies": {
        "System.Threading.Thread": "4.4.0-*"
      }
    }
  }
}