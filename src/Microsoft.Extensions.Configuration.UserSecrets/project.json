{
  "version": "1.2.0-*",
  "buildOptions": {
    "warningsAsErrors": true,
    "keyFile": "../../tools/Key.snk",
    "nowarn": [
      "CS1591"
    ],
    "xmlDoc": true
  },
  "description": "User secrets configuration provider implementation for Microsoft.Extensions.Configuration.",
  "packOptions": {
    "repository": {
      "type": "git",
      "url": "https://github.com/aspnet/Configuration"
    },
    "tags": [
      "configuration",
      "secrets",
      "usersecrets"
    ],
    "files": {
      "include": "build/**/*"
    }
  },
  "dependencies": {
    "Microsoft.Extensions.Configuration.Json": {
      "target": "project"
    },
    "NETStandard.Library": "1.6.2-*"
  },
  "frameworks": {
    "net451": {},
    "netstandard1.5": {}
  }
}